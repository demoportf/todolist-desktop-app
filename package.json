{
  "name": "ProgressiveToDoList",
  "version": "1.0.0",
  "description": "Progressive ToDo List Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . App --platform=linux --arch=x64",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "progressivetodolist",
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "angular-mocks": "^1.7.5",
    "chai": "^4.2.0",
    "electron": "^3.0.0",
    "electron-builder": "^20.28.4",
    "electron-packager": "^12.2.0",
    "jasmine-core": "^3.3.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.20",
    "angular": "^1.7.5",
    "angular-animate": "^1.7.5",
    "angular-aria": "^1.7.5",
    "angular-material": "^1.1.10",
    "angular-messages": "^1.7.5",
    "angular-moment": "^1.3.0",
    "angular-route": "^1.7.5",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "ng-infinite-scroll": "^1.3.0"
  }
}
